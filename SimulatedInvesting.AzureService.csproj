<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Tim Hanewich</Authors>
    <Description>The cloud service adaptation for the SimulatedInvesting NuGet package. Provides cloud storage abilities for any Azure Storage Account by passing a connection string.</Description>
    <PackageIcon>DollarIcon.png</PackageIcon>
    <RepositoryUrl>https://github.com/TimHanewich/SimulatedInvesting.AzureService</RepositoryUrl>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <FileVersion>0.0.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Storage.Blob" Version="11.1.5" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="SimulatedInvesting" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="DollarIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
